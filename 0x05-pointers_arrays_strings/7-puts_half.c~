#include <stdio.h>
#include <string.h>

void puts_half(char *str) {
int len = strlen(str);
int half_len = len / 2;
    
/* If the length of the string is odd, add 1 to the
 * half length to get the midpoint
 */
if (len % 2 != 0) {
half_len += 1;
}
    
/* Print the second half of the string*/
    printf("%s\n", &str[half_len]);
}
